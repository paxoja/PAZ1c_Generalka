<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.cell.PropertyValueFactory?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/20.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.generalka.ShowTestResultController">
    <children>
      <Label fx:id="scoreLabel" layoutX="37.0" layoutY="40.0" prefHeight="29.0" prefWidth="522.0" textAlignment="CENTER" />
        <TableView fx:id="resultsTable" layoutX="39.0" layoutY="80.0" prefHeight="284.0" prefWidth="522.0">
            <columns>
                <TableColumn prefWidth="173.0" text="Question Number">
                    <cellValueFactory>
                        <PropertyValueFactory property="questionNumber" />
                    </cellValueFactory>
                </TableColumn>
                <TableColumn prefWidth="173.0" text="Your Answer">
                    <cellValueFactory>
                        <PropertyValueFactory property="userAnswer" />
                    </cellValueFactory>
                </TableColumn>
                <TableColumn prefWidth="175.0" text="Correct Answer">
                    <cellValueFactory>
                        <PropertyValueFactory property="correctAnswer" />
                    </cellValueFactory>
                </TableColumn>
            </columns>
        </TableView>
        <Label layoutX="254.0" layoutY="14.0" text="Test Results">
            <font>
                <Font size="18.0" />
            </font>
        </Label>
      <Button fx:id="returnToGeneralkaButton" layoutX="39.0" layoutY="42.0" mnemonicParsing="false" onAction="#returnToGeneralka" prefHeight="26.0" prefWidth="100.0" style="-fx-background-color: #423B63; -fx-text-fill: white;" text="Return" />
    </children>
</AnchorPane>
