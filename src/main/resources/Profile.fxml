<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" style="-fx-background-color: #a290f3;" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.generalka.ProfileController">
   <children>

      <Button fx:id="returnToGeneralkaButton" layoutX="24.0" layoutY="23.0" mnemonicParsing="false" onAction="#returnToGeneralka" prefHeight="26.0" prefWidth="100.0" style="-fx-background-color: #423B63; -fx-text-fill: white;" text="Return" />

      <Label fx:id="profileUsername" layoutX="24.0" layoutY="76.0" style="-fx-font-weight: bold;" text="Your Username" textFill="WHITE">
         <font>
            <Font name="Arial" size="36.0" />
         </font>
      </Label>
      <Text fill="WHITE" layoutX="24.0" layoutY="276.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Tests">
         <font>
            <Font name="Arial" size="18.0" />
         </font>
      </Text>
      <ImageView fitHeight="158.0" fitWidth="160.0" layoutX="386.0" layoutY="79.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@profile.jpg" />
         </image>
      </ImageView>
      <ScrollPane layoutX="24.0" layoutY="281.0" prefHeight="94.0" prefWidth="518.0">
        <content>
          <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="112.0" prefWidth="200.0" />
        </content>
      </ScrollPane>
      <Text fill="WHITE" layoutX="27.0" layoutY="142.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Age:">
         <font>
            <Font name="Arial" size="18.0" />
         </font>
      </Text>
      <Text fill="WHITE" layoutX="25.0" layoutY="174.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Country:">
         <font>
            <Font name="Arial" size="18.0" />
         </font>
      </Text>
      <Text fill="WHITE" layoutX="25.0" layoutY="204.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Gender:">
         <font>
            <Font name="Arial" size="18.0" />
         </font>
      </Text>
      <Label fx:id="ageLabel" layoutX="78.0" layoutY="126.0" textFill="WHITE">
         <font>
            <Font name="Arial" size="18.0" />
         </font>
      </Label>
      <Label fx:id="countryLabel" layoutX="100.0" layoutY="157.0" textFill="WHITE">
         <font>
            <Font name="Arial" size="18.0" />
         </font>
      </Label>
      <Label fx:id="genderLabel" layoutX="99.0" layoutY="187.0" textFill="WHITE">
         <font>
            <Font name="Arial" size="18.0" />
         </font>
      </Label>
   </children>
</AnchorPane>
